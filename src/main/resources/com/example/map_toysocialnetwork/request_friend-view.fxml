<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.map_toysocialnetwork.controller.RequestFriendController">
    <children>
        <TextField fx:id="searchBar" promptText="Caută utilizator după nume..." layoutX="14.0" layoutY="14.0" prefWidth="300.0"/>
        <Button fx:id="searchButton" text="Caută" layoutX="324.0" layoutY="14.0" onAction="#handleSearch"/>
        <TableView fx:id="tableViewUsers" layoutX="14.0" layoutY="50.0" prefWidth="400.0" prefHeight="300.0">
            <columns>
                <TableColumn fx:id="UsernameColumn" text="Username" prefWidth="100.0"/>
                <TableColumn fx:id="FirstNameColumn" text="Prenume" prefWidth="150.0"/>
                <TableColumn fx:id="LastNameColumn" text="Nume" prefWidth="150.0"/>
            </columns>
        </TableView>
        <Button fx:id="sendRequestButton" text="Trimite cerere" layoutX="14.0" layoutY="370.0" onAction="#handleSendRequest"/>
    </children>
</AnchorPane>
